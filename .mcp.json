{
  "mcpServers": {
    "context7": {
      "type": "http",
      "url": "https://mcp.context7.com/mcp"
    },
    "sequential-thinking": {
      "command": "cmd",
      "args": ["/c", "npx", "-y", "@modelcontextprotocol/server-sequential-thinking"]
    },
    "supabase": {
      "type": "http",
      "url": "https://mcp.supabase.com/mcp",
      "description": "Supabase PostgreSQL database operations - OAuth authentication",
      "enabled": true
    },
    "vercel": {
      "type": "http",
      "url": "https://mcp.vercel.com",
      "description": "Vercel deployment and hosting management - OAuth authentication",
      "enabled": true
    },
    "shadcn": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "shadcn@latest",
        "mcp"
      ],
      "enabled": true
    }
  }
}
