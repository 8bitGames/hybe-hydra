{
  "name": "@anthropic/agent-framework",
  "version": "0.1.0",
  "description": "A reusable AI agent framework for building intelligent, self-improving agents",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/types/index.mjs",
      "require": "./dist/types/index.js"
    },
    "./core": {
      "types": "./dist/core/index.d.ts",
      "import": "./dist/core/index.mjs",
      "require": "./dist/core/index.js"
    },
    "./models": {
      "types": "./dist/models/index.d.ts",
      "import": "./dist/models/index.mjs",
      "require": "./dist/models/index.js"
    },
    "./adapters": {
      "types": "./dist/adapters/index.d.ts",
      "import": "./dist/adapters/index.mjs",
      "require": "./dist/adapters/index.js"
    },
    "./clients": {
      "types": "./dist/clients/index.d.ts",
      "import": "./dist/clients/index.mjs",
      "require": "./dist/clients/index.js"
    },
    "./examples": {
      "types": "./dist/examples/index.d.ts",
      "import": "./dist/examples/index.mjs",
      "require": "./dist/examples/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/",
    "clean": "rm -rf dist",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "ai",
    "agent",
    "llm",
    "gemini",
    "openai",
    "anthropic",
    "framework",
    "typescript"
  ],
  "author": "Hybe",
  "license": "MIT",
  "peerDependencies": {
    "@google/genai": "^0.10.0",
    "zod": "^3.22.0 || ^4.0.0"
  },
  "peerDependenciesMeta": {
    "@google/genai": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@vitest/coverage-v8": "^4.0.16",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^4.0.16",
    "zod": "^3.22.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/anthropics/agent-framework.git"
  },
  "publishConfig": {
    "access": "public"
  }
}
